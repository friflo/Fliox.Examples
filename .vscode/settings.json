{
    "files.exclude": {
        "docs": true,
        ".gitignore": true,
        ".gitpod.yml": true,
		"**/obj": true,
		"**/bin": true,
        "**/.github": false,
        "**/.vs": true,
        "**/.vscode": false,
        "**/.run": true,
        "**/.idea": true,
        "**/*.user": true,
        "**/*.csproj": true,
        "**/*.DotSettings": true,
        "**/appsettings.json": true,
        "Demo/Client/README.md": true,
        "Todo/Client/README.md": true,
        "package.json": false,
        "package-lock.json": true,
        "node_modules": true,
        "scripts": true,
    },
    "cSpell.words": [
        "bchavez",
        "cref",
        "demohub",
        "DTO",
        "GraphiQL",
        "LINQ",
        "mdast",
        "Npgsql",
        "Randomizer",
        "Swashbuckle",
        "todohub",
        "Ullrich",
        "Unicodes"
    ],
    "csharp.unitTestDebuggingOptions": {
        "logging": {
            "moduleLoad": false
        }
    },
    "explorer.compactFolders": false,
    "[markdown]": {
        "editor.wordWrap": "off"
    }
}
{
    "id": "user_db-allow",
    "description": "Allow unrestricted access to user_db: permissions & roles. Allow execution of the given commands",
    "rights": [
        {
            "type": "operation",
            "database": "user_db",
            "containers": [
                {
                    "name": "permissions",
                    "operations": [
                        "full"
                    ]
                },
                {
                    "name": "roles",
                    "operations": [
                        "full"
                    ]
                }
            ]
        },
        {
            "type": "sendMessage",
            "database": "user_db",
            "names": [
                "std.*",
                "ValidateUserDb"
            ]
        }
    ]
}